<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <link rel="icon" href="../../../../favicon.ico" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD"
      crossorigin="anonymous"
    />

    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css"
      integrity="sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link
      href="https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css"
      rel="stylesheet"
    />
    <link rel="stylesheet" href="assets/css/style.css" />

    <title>B57Personalweb</title>

    <link href="./assets/css/carousel.css" rel="stylesheet" />
    <style>
      @media only screen and (max-width: 1280px) { /* only size 'xs' and below
      */ .searchResults { height: 58%; } } @media only screen and (max-width:
      680px) { /* only size 'xs' and below */ .searchResults { height: 100%; } }
      .text-log { width: clamp(300px, 10%, 400px); }
    </style>
  </head>
  <body>
    <header data-bs-theme="dark">
      <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-white">
        <div class="container-fluid">
          <img
            src="https://dumbways.id/assets/images/brandred.png"
            class="navbar-brand navbar-logo"
          />
          <button
            class="navbar-toggler text-dark"
            type="button"
            data-bs-toggle="collapse"
            data-bs-target="#navbarCollapse"
            aria-controls="navbarCollapse"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <span class="navbar-toggler-icon"><i
                style="padding-top: 5px"
                class="fa fa-bars"
                aria-hidden="true"
              ></i></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarCollapse">
            <ul class="navbar-nav me-auto mb-2 mb-md-0">
              <li class="nav-item active">
                <a class="nav-link text-dark poppins-bold" href="/">Home
                  <span class="sr-only">(current)</span></a>
              </li>
              <li class="nav-item">
                <a
                  class="nav-link text-dark poppins-regular"
                  href="/project"
                >Add Project</a>
              </li>
              <li class="nav-item">
                <a
                  class="nav-link text-dark poppins-regular"
                  href="/testimonial"
                >Testimonial</a>
              </li>
            </ul>
            <div>
              <ul class="navbar-nav me-auto mb-2 mb-md-0">
                {{#if user}}
                  {{!-- <h1>Hello, {{user.name}}</h1> --}}
                  <li class="nav-item">
                    <p
                      class="nav-link text-dark poppins-regular mt-2"
                      href="/testimonial"
                    >Welcome back!, <strong>{{user.name}}</strong></p>
                  </li>
                  <li class="nav-item m-2">
                    <a
                      href="/logout"
                      class="btn btn-danger poppins-regular p-2"
                    >Logout</a>

                  </li>
                {{else}}
                  <li class="nav-item m-2">
                    <a
                      href="/register"
                      class="btn btn-dark poppins-regular p-2"
                    >Register</a>

                  </li>
                  <li class="nav-item m-2">
                    <a
                      href="/login"
                      class="btn btn-dark poppins-regular p-2"
                    >Login</a>
                  </li>

                {{/if}}
                <li class="nav-item m-2">
                  <a
                    class="btn btn-dark poppins-regular"
                    href="/contact"
                    style="padding: 10px"
                  >
                    Contact Me
                  </a>
                </li>

              </ul>

            </div>
          </div>
        </div>
      </nav>
    </header>

    <main role="main">
      <div class="container">
        <div class="jumbotron p-3 p-md-5 text-white rounded"></div>
        {{#if messages.error}}
          <div class="alert alert-danger" role="alert">
            {{messages.error}}
          </div>
        {{/if}}
        {{#if messages.success}}
          <div class="alert alert-success" role="alert">
            {{messages.success}}
          </div>
        {{/if}}
        <div class="row mb-2">
          <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="col-md-8 col-lg-9 col-xl-9">
              <div class="card-body d-flex flex-column align-items-start">
                <div>
                  <h1 class="poppins-bold" style="text-align: center">
                    Welcome to my personal website
                  </h1>
                  <p class="poppins-regular">
                    A Full Stack Node.js Developer is a highly skilled professional adept at both front-end and back-end development. 
                    They leverage JavaScript and Node.js to build robust, scalable web applications, ensuring seamless user experiences and 
                    efficient server-side operations. Their expertise spans across various technologies, including databases, APIs, and front-end frameworks, 
                    enabling them to create comprehensive, end-to-end solutions. This versatility makes them invaluable in modern web development projects.
                  </p>
                  <div class="d-flex flex-row mb-3">
                    <div class="p-2">
                      <button
                        class="poppins-semibold"
                        style="
                          background-color: black;
                          border-radius: 20px;
                          color: white;
                          width: 100px;
                          height: 35px;
                        "
                      >
                        contact
                      </button>
                    </div>
                    <div class="p-2 mt-2">
                      <p class="poppins-semibold">
                        download cv
                        <i class="fa fa-download" aria-hidden="true"></i>
                      </p>
                    </div>
                  </div>
                  <div class="social-icons">
                    <a href="#" title="instagram">
                      <i class="fa fa-instagram" aria-hidden="true"></i>
                    </a>
                    <a href="#" title="youtube">
                      <i class="fa fa-youtube-square" aria-hidden="true"></i>
                    </a>
                    <a href="#" title="linkedin">
                      <i class="fa fa-linkedin-square" aria-hidden="true"></i>
                    </a>
                  </div>
                </div>
              </div>
            </div>
            <div
              class="col-md-2 col-xl-3 pt-5 d-none d-xl-block"
              style="height: 360px"
            >
              <div class="card text-center h-100" style="width: 18rem">
                <img src="assets/img/img1.jpg" class="card-img-top" alt="..." />
                <div class="card-body">
                  <h5 class="card-title poppins-bold">
                    Gerald Ghibran Gunasha
                  </h5>
                  <p class="card-text poppins-regular">Fullstack Developer</p>
                </div>
              </div>
            </div>
            <div
              class="col-md-4 col-xl-3 pt-5 d-flex justify-content-center mb-5"
            >
              <div
                class="card text-center d-block d-xl-none searchResults"
                style="width: 18rem"
              >
                <img src="assets/img/img1.jpg" class="card-img-top" alt="..." />
                <div class="card-body">
                  <h5 class="card-title poppins-bold">
                    Gerald Ghibran Gunasha
                  </h5>
                  <p class="card-text poppins-regular">Fullstack Developer</p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="container marketing">
        <div class="row">

          <div id="contents" class="blog-list">
            <div class="row gx-2">
              {{#each data}}
                <div class="col-sm-1 col-md-6 col-lg-3 col-xl-3">
                  <div class="p-2">
                    <div class="card d-flex card-all">
                      <div
                        class="card-body d-flex"
                        style=" height: 11rem; width: 16rem;"
                      >
                        <a
                          href="/project-detail/{{id}}"
                          class="card-title fw-bold"
                        >
                          <img
                            src="{{image}}"
                            class="card-img-top d-flex sm:justify-content-center"
                            alt="Image"
                          />
                        </a>
                      </div>
                      <ul class="list-group list-group-flush">
                        <li class="list-group-item">
                          <a
                            href="/project-detail/{{id}}"
                            class="card-title fw-bold"
                          >{{projectName}}</a>
                        </li>
                        <p class="list-group-item h-10">
                          {{description}}
                        </p>
                        <p class="list-group-item h-10">
                          {{author}}
                        </p>
                        
                      </ul>

                      {{#if ../user}}
                        <div class="card-body text-center">
                          <a
                            href="/edit-project/{{id}}"
                            class="btn btn-dark mx-2"
                          >Edit Post</a>
                          <a
                            href="/delete-project/{{id}}/imageId/{{imageId}}"
                            class="btn btn-dark"
                          >Delete Post</a>
                        </div>

                      {{/if}}
                     

                    </div>
                  </div>
                </div>
              {{/each}}
            </div>
          </div>
          <!-- navbar end -->

          <!-- Edit Module End -->
        </div>
      </div>
    </main>

    <script
      src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
      integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
      crossorigin="anonymous"
    ></script>
    <script>
      window.jQuery || document.write( '<script
      src="./assets/js/vendor/jquery-slim.min.js"><\/script>' );
    </script>
    <script src="./assets/js/vendor/popper.min.js"></script>
    <!-- <script src="./assets/js/bootstrap.min.js"></script> -->
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
      crossorigin="anonymous"
    ></script>
    <!-- Just to make our placeholder images work. Don't actually copy the next line! -->
    <script src="./assets/js/vendor/holder.min.js"></script>
    <script
      src="https://kit.fontawesome.com/66aa7c98b3.js"
      crossorigin="anonymous"
    ></script>
    <script src="https://unpkg.com/boxicons@2.1.4/dist/boxicons.js"></script>
    <script src="assets/js/script.js"></script>
  </body>
</html>